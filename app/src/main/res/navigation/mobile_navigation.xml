<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/mobile_navigation"
  app:startDestination="@+id/navigation_home">

  <fragment
    android:id="@+id/navigation_home"
    android:name="com.example.seatinalpha.ui.home.HomeFragment"
    android:label="@string/title_home"
    tools:layout="@layout/fragment_home" />

  <fragment
    android:id="@+id/navigation_my_webinar"
    android:name="com.example.seatinalpha.ui.mywebinar.MyWebinarFragment"
    android:label="My Webinar"
    tools:layout="@layout/fragment_my_webinar" />

  <fragment
    android:id="@+id/navigation_feed"
    android:name="com.example.seatinalpha.ui.feed.FeedFragment"
    android:label="Feed"
    tools:layout="@layout/fragment_feed" />

  <fragment
    android:id="@+id/navigation_account"
    android:name="com.example.seatinalpha.ui.account.AccountFragment"
    android:label="Account"
    tools:layout="@layout/fragment_account" />

  <fragment
    android:id="@+id/navigation_history"
    android:name="com.example.seatinalpha.ui.history.HistoryFragment"
    android:label="History"
    tools:layout="@layout/fragment_history" />
</navigation>